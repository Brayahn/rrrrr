  "name": "TEST",
  "prepared_report": 0,
  "query": "SELECT\n    student AS \"Student:Link/Student:200\",\n    std.student_name AS \"Student Name::200\",\n    CASE \n        WHEN std.joining_date BETWEEN '2022-01-01' AND '2024-01-01' THEN std.joining_date\n        ELSE NULL \n    END AS 'Joining Date',\n    enr.program AS 'Course',\n    enr.enrollment_date AS 'Enrollment Date',\n    CASE \n        WHEN enr.enrollment_date BETWEEN '2022-01-01' AND '2024-01-01' THEN enr.enrollment_date\n        ELSE NULL \n    END AS 'Enrollment Date'\nFROM\n    `tabCourse Enrollment` AS enr\nJOIN\n    `tabStudent` AS std ON std.name = enr.student\nWHERE\n    enr.docstatus = 0\n    AND std.joining_date BETWEEN '2022-01-01' AND '2024-01-01'\n    AND enr.enrollment_date BETWEEN '2022-01-01' AND '2024-01-01'\nGROUP BY\n    student;\n",
